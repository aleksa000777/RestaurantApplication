<%= form_for @party do |f| %>
<!-- <%# binding.pry %> -->

  <%= f.collection_select :table_id, Table.all, :id, :name, include_blank: true %>
<%# binding.pry %>
  <%= f.collection_select :server_id, Server.all, :id, :name, include_blank: true %>
  <!-- <%#= f.label :name, "name" %>
  <%#= f.text_field :name %><br> -->
  <%= f.label :nmofpeople, "Number of people" %>
  <%= f.number_field :nmofpeople %><br>
  <%= label :pay_status, "no paid" %>
  <%= f.radio_button :pay_status, false , :checked => true , :value => false %><br>
  <%= label :pay_status, "paid"%>
  <%= f.radio_button :pay_status, true, :value => true%><br>
  <%= f.label :mood, "mood" %>
  <%= f.text_field :mood %><br>

  <%-# Server: <%= current_server.name <br> -%>
  <%= f.submit %>

<% end %>


